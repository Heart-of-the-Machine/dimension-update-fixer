{
  "schemaVersion": 1,
  "id": "dimensionupdatefixer",
  "version": "${version}",
  "name": "Dimension Update Fixer",
  "description": "A simple mod that unyeets the Nether and End when a world is loaded without a dimension or is upgraded between Minecraft versions with dimension mods installed",
  "authors": [
    "Kneelawk",
    "alcatrazEscapee"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/Heart-of-the-Machine/dimension-update-fixer"
  },
  "license": "MIT",
  "icon": "assets/dimensionupdatefixer/icon.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "com.github.hotm.dimensionupdatefixer.DimensionUpdateFixerPreLaunch"
    ],
    "main": [
    ]
  },
  "mixins": [
    "dimensionupdatefixer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.9.2",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "hotm": "*"
  }
}
